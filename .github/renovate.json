{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "baseBranches": ["main"],
  "extends": [
    ":separateMajorReleases",
    ":rebaseStalePrs",
    ":disableRateLimiting",
    ":docker",
    ":semanticCommits",
    "group:allNonMajor"
  ],
  "enabledManagers": ["github-actions"],
  "commitMessageAction": "VERB",
  "commitMessageTopic": "{{depName}}",
  "commitMessageExtra": ""
}
